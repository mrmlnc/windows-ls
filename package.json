{
  "name": "windows-ls",
  "version": "0.1.3",
  "description": "Simple implementation of the ls command for windows",
  "license": "MIT",
  "repository": "mrmlnc/windows-ls",
  "author": {
    "name": "Denis Malinochkin",
    "url": "canonium.com"
  },
  "bin": {
    "ls": "./cli.js"
  },
  "scripts": {
    "test": "xo --ignore=test/fixtures/** && node test/test.js"
  },
  "files": [
    "README.md",
    "LICENSE",
    "cli.js",
    "lib/*"
  ],
  "keywords": [
    "windows",
    "ls",
    "win",
    "list",
    "linux",
    "unix",
    "command",
    "cli",
    "implementation",
    "implement"
  ],
  "devDependencies": {
    "ava": "^0.1.0",
    "xo": "^0.8.0"
  },
  "dependencies": {
    "chalk": "^1.1.1",
    "dateformat": "^1.0.11",
    "glob": "^5.0.14",
    "minimist": "^1.2.0",
    "pretty-bytes": "^2.0.1",
    "text-table": "^0.2.0"
  },
  "xo": {
    "space": true,
    "rules": {
      "object-curly-spacing": [
        2,
        "always"
      ],
      "space-before-function-paren": 0
    },
    "envs": [
      "node"
    ]
  }
}
