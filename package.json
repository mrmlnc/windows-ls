{
  "name": "windows-ls",
  "version": "1.0.1",
  "description": "Simple implementation of the ls command for windows",
  "license": "MIT",
  "repository": "mrmlnc/windows-ls",
  "author": {
    "name": "Denis Malinochkin",
    "url": "canonium.com"
  },
  "bin": {
    "ls": "./cli.js"
  },
  "scripts": {
    "test": "xo --ignore=test/fixtures/** && mocha test/test.js"
  },
  "files": [
    "cli.js",
    "lib"
  ],
  "keywords": [
    "windows",
    "ls",
    "win",
    "list",
    "linux",
    "unix",
    "command",
    "cli",
    "implementation",
    "implement"
  ],
  "devDependencies": {
    "mocha": "^3.3.0",
    "xo": "^0.18.2"
  },
  "dependencies": {
    "chalk": "^1.1.1",
    "dateformat": "^2.0.0",
    "glob": "^7.1.1",
    "has-glob": "^1.0.0",
    "minimist": "^1.2.0",
    "pretty-bytes": "^4.0.2",
    "text-table": "^0.2.0"
  },
  "xo": {
    "space": true,
    "rules": {
      "arrow-parens": [
        2,
        "always"
      ],
      "object-curly-spacing": [
        2,
        "always"
      ],
      "space-before-function-paren": [
        2,
        "never"
      ]
    },
    "envs": [
      "node",
      "mocha"
    ]
  }
}
